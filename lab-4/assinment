bit
#include<iostream>
using namespace std;
int main()
{
    int n,x=0;
    cin>>n;
    string s;
    while(n--)
    {
        cin>>s;
        if(s =="++x"||"s =="x++")
        {
            x++;
        }
        else
        {
            x--;
        }
    }
    cout<<x<<endl;
    return 0;
}

domino
#include <iostream>
using namespace std;
int main() {
 int m,n;
 cin>>m>>n;
 cout<<(m*n)/(2*1)<<endl;
 return 0;
}
